<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view_a_property.MainActivity"
    android:fitsSystemWindows="true">
<com.example.findhome.custom_things.CustomScrollView
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:scrollbars="none">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <fragment
        android:id="@+id/fragment1"
        android:name="com.example.findhome.view_a_property.ImagesCarouselFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        />
    <fragment
        android:id="@+id/fragment2"
        android:name="com.example.findhome.view_a_property.BestChoiceDifferentChoiceFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/fragment1"
        app:layout_constraintStart_toStartOf="parent"
        />
    <fragment
        android:id="@+id/fragment3"
        android:name="com.example.findhome.view_a_property.MapsAndLocationFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/fragment2"
        app:layout_constraintStart_toStartOf="parent"
        />
        <fragment
        android:id="@+id/fragment4"
        android:name="com.example.findhome.view_a_property.RatingsFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/fragment3"
        app:layout_constraintStart_toStartOf="parent"
        />
        <fragment
            android:id="@+id/fragment5"
            android:name="com.example.findhome.view_a_property.ReviewsFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/fragment4"
            app:layout_constraintStart_toStartOf="parent"
            />
        <fragment
            android:id="@+id/fragment6"
            android:name="com.example.findhome.view_a_property.HotelDetailsFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/fragment5"
            app:layout_constraintStart_toStartOf="parent"
            />
        <fragment
            android:id="@+id/fragment7"
            android:name="com.example.findhome.view_a_property.RoomAmenitiesFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/fragment6"
            app:layout_constraintStart_toStartOf="parent"
            />
        <fragment
            android:id="@+id/fragment8"
            android:name="com.example.findhome.view_a_property.HotelAmenitiesFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/fragment7"
            app:layout_constraintStart_toStartOf="parent"
            />
        <fragment
            android:id="@+id/fragment9"
            android:name="com.example.findhome.view_a_property.SuggestedPropertiesFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/fragment8"
            app:layout_constraintStart_toStartOf="parent"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</com.example.findhome.custom_things.CustomScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>